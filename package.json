{
  "name": "emr-api",
  "version": "0.0.9",
  "description": "This is the Official Dental EMR API",
  "author": "BD Computing Limited",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "docker": "docker-compose -f docker-compose-dev.yml up --build",
    "docker:build": "docker-compose build",
    "docker:run": "docker compose run --build -d --name emr-api --env .env --publish 3000:80 -p 3000:80 api redis redis-insight",
    "docker:up": "docker compose up -d",
    "docker:stop": "docker stop $(docker container ls -a -q) && docker compose down",
    "docker:remove": "docker rm $(docker container ls -a -q)",
    "docker:restart": "npm run docker:stop && npm run docker:remove && npm run docker:run",
    "docker:deploy": "npm run docker:build && npm run docker:up",
    "docker:logs": "docker-compose logs",
    "build": "export NODE_OPTIONS=--max_old_space_size=8192 && node ./build/build-pre.cjs && nest build",
    "git-push-version-update": "git fetch && git pull &&  git add . && git commit -m 'chore: updated build version number' && git push",
    "release": "standard-version && node ./build/build-pre.cjs && npm run git-push-version-update && git push --follow-tags origin dev",
    "build:render": "rm -Rf node_modules && rm -f package-lock.json && npm install && npm run build",
    "redis:start": "redis-stack-server",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:nowatch": "TZ=Africa/Nairobi nest start",
    "start": "export NODE_OPTIONS=--max_old_space_size=8192 && TZ=Africa/Nairobi nest start --watch",
    "sw": "set TZ=Africa/Nairobi && nest start --watch",
    "start:debug": "TZ=Africa/Nairobi nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "export NODE_OPTIONS=--max_old_space_size=8192 && TZ=Africa/Nairobi node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "cache:clean": "rm -rf node_modules && rm -rf package-lock.json && npm cache clean --force && npm install"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/bullmq": "^10.1.1",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^9.4.3",
